package lec1;

public class Circle extends Shape {
	// attributes
	private double radius;

	public Circle() {
		this.radius = 1.0;
	}
	
	public Circle(double radius) {
		super();    // the 0 parameter const from Shape is called. It is not necessary to have 
		this.radius = radius;
	}
	
	// constructors
	
	
	// methods
}
